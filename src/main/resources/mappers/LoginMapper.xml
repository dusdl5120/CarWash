<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.green.carwash.dao.LoginMapper">
 
	<!-- 공지사항 -->
	<select id="login" resultType="kr.green.carwash.vo.LoginVO">
		SELECT 
			id
			, passwd
		FROM
			general_account
		WHERE
			id = #{id}
		and
			passwd = #{passwd}
	</select>

	
	<select id="loginById" resultType="kr.green.carwash.vo.LoginVO">
		SELECT 
			id
			, passwd
		FROM
			general_account
		WHERE
			id = #{id}
	</select>
	

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

</mapper>